{
  "name": "Stackmin",
  "description": "Simple Cloudstack Administration Interface",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["js/chrome_app.js"]
    }
  },
  "icons": {
    "64": "icons/icon_64.png",
    "48": "icons/icon_48.png"
  },
  "permissions": [ "https://*/*", "storage", "notifications" ]
}